    "mocha": "^3.2",
    "rimraf": "^2.5.4",
    "typescript": "^2.1.4",
    "vscode": "^1.0.3"
  },
  "scripts": {
    "clean": "rimraf ../client/server",
    "build": "npm run compile",
    "clean-build": "npm run clean && npm run build",
